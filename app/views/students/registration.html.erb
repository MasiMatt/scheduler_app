
  <div id="course-schedule-section" class="btn-group col-xs-7" role="group">
    <button id="current-course-offered-button" type="button" class="btn btn-default col-xs-12">Winter 2016 Courses</button>
    <div id="current-course-offered-table" class="col-xs-12" style="padding-top: 20px">
      <table class="cell-border hover">
        <thead>
          <tr>
            <th>Course Id</th>
            <th>Location</th>
            <th>Professor</th>
            <th>Time</th>
            <th>Turorial Info</th>
            <th>Lab info</th>
          </tr>
        </thead>
        <tbody>
          <% @current_courses.each do |course| %>
            <tr>
              <td><%=course.get_course_id%></td>
              <td><%=course.professor%></td>
              <td><%=course.location%></td>
              <td><%=course.get_lecture_info%></td>
              <td><%=course.get_tutorial_info%></td>
              <td><%=course.get_lab_info%></td>
            </tr>
          <% end %>
        </tbody>
      </table>

    </div>
  </div>
<div class="col-xs-5">
  <div class="btn-group col-xs-7" role="group">
    <button id="auto-generate-schedule-button" type="button" class="btn btn-default col-xs-12">Auto Generate my schedule </button>
  </div>
  <div class="btn-group col-xs-5" role="group">
    <button id="preference-generate-schedule-button" type="button" class="btn btn-default col-xs-12">Preference setting</button>
  </div>
  <div id="auto-generate-schedule" class="col-xs-12">
  </div>
  <div id="preference-generate-schedule" class="col-xs-12">
    <div id="day-option" class="col-xs-12">
      <h3>Day option</h3>
      <select id="weekday-selection">
        <option value="Monday">Monday</option>
        <option value="Tuesday">Tuesday</option>
        <option value="Wednesday">Wednesday</option>
        <option value="Thrusday">Thrusday</option>
        <option value="Friday">Friday</option>
      </select>
    </div>
    <div id="time-option" class="col-xs-12">
      <h3>Time option</h3>
      <select id="time-selection">
        <option value="Morning">Morning</option>
        <option value="Afternoon">Afternoon</option>
        <option value="Evening">Evening</option>
      </select>
    </div>
    <div id="credit-option" class="col-xs-12">
      <h3>Number of class</h3>
      <input id="number-of-class-option"></input>
    </div>
  </div>
  <div class="col-xs-12" style="margin-top: 20px">
    <button id="preference-submit-button">Submit</button>
  </div>
</div>
